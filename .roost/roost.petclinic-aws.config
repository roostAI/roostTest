app_dependencies:
  type: text
  content: |-
    #!/bin/sh

    docker build -f Dockerfile -t petclinic:$GIT_COMMIT_ID . 
    docker tag petclinic:$GIT_COMMIT_ID  petclinic:$ROOST_NAMESPACE  
app_deploy:
  type: text
  content: |
    #!/bin/sh

    docker run  -d -p 30400:8080 --name petclinic petclinic:$ROOST_NAMESPACE
app_test:
  - name: petclinic
    configType: maven
    configPath: /
app_uninstall:
  type: text
  content: |-
    #!/bin/sh

    docker kill petclinic
app_docker_compose: ''
